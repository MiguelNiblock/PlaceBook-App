{
  "expo": {
    "name": "PlaceBook",
    "slug": "PlaceBook",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.miguelniblock.PlaceBook",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDe1__AtFL5ENtGVrYIuXI0AUkewvkVSUg"
        }
      }
    }
  }
}
